<div class='trackpick'>
  <img src="<%= trackpick.track.artwork_url %>"></img>
  <br>

  <div class="soundcloud-url" id="<%= trackpick.track.track_url + '?client_id=' + ENV['CLIENT_ID'] %>"></div>
 
  <p>
    <%= trackpick.track.title %>
  </p>
  <p>
    Added on <%= trackpick.created_at.strftime("%F") %>
  </p>
</div>